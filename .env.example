# Deployment variables (DO NOT COMMIT TO GIT)
# Copy this to .env.local and fill in your values

# Google Apps Script Web App URL (from deployment)
VITE_APPS_SCRIPT_URL=https://script.google.com/macros/s/YOUR_SCRIPT_ID/exec

# Your deployed frontend URL (Netlify/Vercel URL)
VITE_CHECK_IN_BASE_URL=https://your-app.netlify.app

# Development mode (true = use mock data, false = use real backend)
VITE_DEV_MODE=false
